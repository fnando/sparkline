    "@babel/preset-env": "7.0.x",
    "@babel/register": "7.0.x",
    "@babel/runtime": "7.0.x",
    "babel-loader": "8.0.x",
    "jsdom": "11.7.x",
    "mocha": "5.0.x",
    "snap-shot": "2.17.x",
    "uglifyjs-webpack-plugin": "2.0.x",
    "webpack": "4.19.x",
    "webpack-cli": "3.1.x"
